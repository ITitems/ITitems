<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

    <formset>
		<form name="assetRegist">
			<field property="largeCategory" depends="required">
        		<arg0 key="대분류" resource="false"/>
			</field>			
			<field property="middleCategory" depends="required">
        		<arg0 key="중분류" resource="false"/>
			</field> 
			<field property="assetQty" depends="required, integer">
        		<arg0 key="수량" resource="false"/>
			</field>
			<field property="acquiredDate" depends="required, date">
        		<arg0 key="취득일자" resource="false"/>
			</field>
			<field property="photo" depends="required">
        		<arg0 key="사진" resource="false"/>
			</field>
			<field property="file" depends="required">
        		<arg0 key="지급확인서" resource="false"/>
			</field>
			<field property="rcptId" depends="required">
        		<arg0 key="수령자" resource="false"/>
			</field>
			<field property="useId" depends="required">
        		<arg0 key="실사용자" resource="false"/>
			</field>
			<field property="rcptDate" depends="required, date">
        		<arg0 key="수령일자" resource="false"/>
			</field>
		</form>
    </formset>

</form-validation>